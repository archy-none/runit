proto add = Int, Int -> Int in
proto main = Str, Int -> Str in
let add a, b = (
    let c = a + b in
    if c > 0 then c else (let d = 1 in d)
) in
let main a, b = (
    let n = add 1 + 2, (add b, -3) in
    let c = "Hello, " in
    let d = c + a + "! " in
    let e = c + d in
    e + d * n
) in
main "world", 3
